<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.topupit.asef.autismapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mealActivities.Breakfast" />
        <activity android:name=".mealActivities.Lunch" />
        <activity android:name=".mealActivities.Snacks" />
        <activity android:name=".mealActivities.Dinner" />
        <activity android:name=".scheduleActivities.SundayActivity" />
        <activity android:name=".scheduleActivities.MondayActivity" />
        <activity android:name=".scheduleActivities.TuesdayActivity" />
        <activity android:name=".scheduleActivities.WednesdayActivity" />
        <activity android:name=".scheduleActivities.ThursdayActivity" />
        <activity android:name=".scheduleActivities.FridayActivity" />
        <activity android:name=".scheduleActivities.SaturdayActivity"></activity>
    </application>

</manifest>